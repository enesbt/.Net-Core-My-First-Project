@model EntityLayer.Concrete.City
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "AddCity";
    Layout = "~/Views/Shared/Layouts/_AdminLayout.cshtml";
}

<h6>Şehir Ekle</h6>

<form method="post" class="form-group">
    <label>Şehir Adı</label>
    <input asp-for="CityName" class="form-control" />
    <input type="submit" value="Ekle" class="btn btn-outline-dark" />
</form>