@model List<EntityLayer.Concrete.PetBreed>

<div class="tech-btm">
	<h4>Cinsler</h4>
	<ul class="list-group single">

		@foreach (var item in Model)
		{
			<li class="list-group-item d-flex justify-content-between align-items-center">
				<a href="/Default/GetListByBreed/@item.PetBreedId">@item.PetBreedName</a>
			</li>
		}

	</ul>
</div>